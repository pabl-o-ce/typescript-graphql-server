{
  "name": "typescript-graphql-server",
  "version": "0.0.0",
  "description": "graphql server boilerplate",
  "main": "src/index.ts",
  "scripts": {
    "start": "docker-compose up -d",
    "down": "docker-compose down",
    "build:watch": "webpack -w",
    "build": "webpack",
    "app": "docker logs api --follow",
    "commit": "npx git-cz",
    "travis-deploy-once": "travis-deploy-once --pro",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablocarreraest/typescript-graphql-server.git"
  },
  "keywords": [
    "graphql"
  ],
  "author": "Pablo Carrera Estrada <pcarrerae@carreraestrada.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pablocarreraest/typescript-graphql-server/issues"
  },
  "homepage": "https://github.com/pablocarreraest/typescript-graphql-server#readme",
  "dependencies": {
    "apollo-server": "^2.1.0",
    "apollo-server-cache-redis": "^0.1.3",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "merge-graphql-schemas": "^1.5.7",
    "redis": "^2.8.0",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
